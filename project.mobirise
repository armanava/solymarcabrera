          "_customHTML": "<section class=\"teams2\" group=\"Teams\" plugins=\"amp-main\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header mbr-if=\"showTitle\">Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n\n    <header>Cards</header>\n        <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\">\n        <select title=\"Align\" name=\"cardAlign\" condition=\"showName || showSpecialty || showText || showExperience || showEmail || showTel || showSocial\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Name\" name=\"showName\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Specialty\" name=\"showSpecialty\" checked=\"\">\n        <input type=\"checkbox\" title=\"Line Specialty\" name=\"lineSpecialty\" condition=\"showSpecialty\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Experience\" name=\"showExperience\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Email\" name=\"showEmail\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Telephone\" name=\"showTel\">\n        <input type=\"checkbox\" title=\"Show Social\" name=\"showSocial\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Count Social\" name=\"iconsCount\" min=\"1\" max=\"6\" step=\"1\" value=\"1\" condition=\"showSocial\">\n        <input type=\"range\" inline=\"\" title=\"Size Social\" name=\"iconsSize\" min=\"0.8\" max=\"4\" step=\"0.1\" value=\"2.7\" condition=\"showSocial\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Padding\" name=\"showPadding\" checked=\"\">\n        <input type=\"checkbox\" title=\"Borders\" name=\"showBorders\">\n        <input type=\"checkbox\" title=\"Top Border\" name=\"showTopBorder\" condition=\"showBorders &amp;&amp; !showPadding\">\n        <input type=\"color\" title=\"Color\" name=\"bordersColor\" value=\"#c1c1c1\" condition=\"showBorders\">\n        <input type=\"checkbox\" title=\"Transparent\" name=\"transparentBg\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#f4f5f7\" condition=\"!transparentBg\">\n\n    <header>Image</header>\n        <input type=\"checkbox\" title=\"Rounded\" name=\"roundedImage\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Width\" name=\"imageWidth\" min=\"10\" max=\"100\" step=\"1\" value=\"59\">\n\n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#f4f5f7\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"title-wrap mbr-pb-4\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left': titleAlign==1,\n                         'align-center': titleAlign==2,\n                         'align-right': titleAlign==3, 'mbr-px-2': fullWidth}\">\n            <h3 class=\"mbr-section-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-6\" mbr-if=\"showTitle\" data-app-selector=\".title-wrap .mbr-section-title\">\n                Nuestro Equipo de Odontologos.</h3>\n            <h4 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".title-wrap .mbr-section-subtitle\">\n                Team Subtitle</h4>\n        </div>\n        <div class=\"mbr-row mbr-jc-c\" mbr-class=\"{'mbr-px-2': (fullWidth &amp;&amp; cardsAmount != 1) || !showPadding, 'cardsAmount2': showPadding &amp;&amp; cardsAmount == 2}\">\n            <div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-xl-6':cardsAmount == 2, 'mbr-col-md-4':cardsAmount == 3, 'mbr-col-md-6 mbr-col-lg-3 md-pb':cardsAmount == 4, 'mbr-col-md-12 last-child': cardsAmount == 1}\">\n                <div class=\"card-wrapper\" mbr-class=\"{'align-center': cardAlign==2,\n                         'align-right': cardAlign==3, 'mbr-p-2': !transparentBg || showBorders}\">\n                    <div class=\"card-img\">\n                        <amp-img class=\"mobirise-loader\" mbr-class=\"{'rounded': roundedImage, 'mbr-m-auto': cardAlign==2, 'mbr-ml-auto': cardAlign==3}\" src=\"@PROJECT_PATH@/assets/images/doctora-277x182.jpg\" layout=\"responsive\" width=\"182.63736263736263\" height=\"120\" alt=\"Mobirise\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/doctora-277x182.jpg\" alt=\"Mobirise\" title=\"\">\n                        </amp-img>\n                    </div>\n                    <div class=\"card-box mbr-pt-1\" mbr-if=\"showName || showSpecialty || showText || showExperience || showEmail || showTel || showSocial\">\n                        <h3 class=\"card-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showName\" data-app-selector=\".card-title\" mbr-class=\"{'mbr-pb-1':showSpecialty}\">\n                            Dra. Anabel Amaro</h3>\n                        <h4 class=\"card-subtitle mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSpecialty\" data-app-selector=\".card-subtitle\">Inplntología</h4>\n                        <p class=\"card-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".card-text\" mbr-class=\"{'mbr-pt-2': showSpecialty || showName }\">\n                            Especialista en implatología dental y extracciones dentales. Exodoncia</p>    \n                        <div class=\"info\" mbr-if=\"showExperience || showEmail || showTel\" mbr-class=\"{'mbr-pt-2': showSpecialty || showName || showText}\">\n                            <p class=\"card-experience mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showExperience\" data-app-selector=\".card-experience\" mbr-class=\"{'mbr-pb-1': showEmail || showTel}\"><b>Experiencia:</b> 10 Años</p>\n                            <p class=\"card-email mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showEmail\" data-app-selector=\".card-email\" mbr-class=\"{'mbr-pb-1': showTel}\"><b>Email:</b>&nbsp;info@armanava.com</p>\n                            <p class=\"card-tel mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showTel\" data-app-selector=\".card-tel\"><b>Tel:</b> +(1)000-000-0000</p>\n                        </div>\n                        <div class=\"icons-list social\" mbr-list=\"\" mbr-list-grow=\"iconsCount\" mbr-if=\"showSocial\" mbr-class=\"{'mbr-pt-2': showSpecialty || showName || showExperience || showEmail || showTel || showText}\">\n                            <a href=\"https://www.facebook.com/armanava01\" target=\"_blank\">\n                                <span class=\"iconfont-wrapper\">\n                                    <span mbr-icon=\"\" class=\"amp-iconfont fa-facebook-square fa\"></span>\n                                </span>\n                            </a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <!--  -->\n            <div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-xl-6 last-child':cardsAmount == 2, 'mbr-col-md-4':cardsAmount == 3, 'mbr-col-md-6 mbr-col-lg-3 md-pb':cardsAmount == 4, 'mbr-col-md-6': cardsAmount < 3}\" mbr-if=\"cardsAmount > 1\">\n                <div class=\"card-wrapper\" mbr-class=\"{'align-center': cardAlign==2,\n                         'align-right': cardAlign==3, 'mbr-p-2': !transparentBg || showBorders}\">\n                    <div class=\"card-img\">\n                        <amp-img class=\"mobirise-loader\" mbr-class=\"{'rounded': roundedImage, 'mbr-m-auto': cardAlign==2, 'mbr-ml-auto': cardAlign==3}\" src=\"@PROJECT_PATH@/assets/images/doctor-1149149-341-510x340.jpg\" layout=\"responsive\" width=\"182.5514705882353\" height=\"122\" alt=\"Mobirise\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/doctor-1149149-341-510x340.jpg\" alt=\"Mobirise\" title=\"\">\n                        </amp-img>\n                    </div>\n                    <div class=\"card-box mbr-pt-1\" mbr-if=\"showName || showSpecialty || showText || showExperience || showEmail || showTel || showSocial\">\n                        <h3 class=\"card-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showName\" data-app-selector=\".card-title\" mbr-class=\"{'mbr-pb-1':showSpecialty}\">\n                            Dr. Anibal Lopez</h3>\n                        <h4 class=\"card-subtitle mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSpecialty\" data-app-selector=\".card-subtitle\">Higienista Dental</h4>\n                        <p class=\"card-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".card-text\" mbr-class=\"{'mbr-pt-2': showSpecialty || showName }\">Experto en prevención de enfermedades bucodentales&nbsp;</p>    \n                        <div class=\"info\" mbr-if=\"showExperience || showEmail || showTel\" mbr-class=\"{'mbr-pt-2': showSpecialty || showName || showText}\">\n                            <p class=\"card-experience mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showExperience\" data-app-selector=\".card-experience\" mbr-class=\"{'mbr-pb-1': showEmail || showTel}\">Experiencia: 15 Años</p>\n                            <p class=\"card-email mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showEmail\" data-app-selector=\".card-email\" mbr-class=\"{'mbr-pb-1': showTel}\"><b>Email:</b> info@armanava.com</p>\n                            <p class=\"card-tel mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" mbr-if=\"showTel\" data-app-selector=\".card-tel\"><b>Tel:</b> +(1)000-000-0000</p>\n                        </div>\n                        <div class=\"icons-list social\" mbr-list=\"\" mbr-list-grow=\"iconsCount\" mbr-if=\"showSocial\" mbr-class=\"{'mbr-pt-2': showSpecialty || showName || showExperience || showEmail || showTel || showText}\">\n                            <a href=\"https://www.facebook.com/armanava01\" target=\"_blank\">\n                                <span class=\"iconfont-wrapper\">\n                                    <span mbr-icon=\"\" class=\"amp-iconfont fa-facebook-square fa\"></span>\n                                </span>\n                            </a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <!--  -->\n            <div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-4 last-child':cardsAmount == 3, 'mbr-col-md-6 mbr-col-lg-3':cardsAmount == 4}\" mbr-if=\"cardsAmount > 2\">\n                <div class=\"card-wrapper\" mbr-class=\"{'align-center': cardAlign==2,\n                         'align-right': cardAlign==3, 'mbr-p-2': !transparentBg || showBorders}\">\n                    <div class=\"card-img\">\n                        <amp-img class=\"mobirise-loader\" mbr-class=\"{'rounded': roundedImage, 'mbr-m-auto': cardAlign==2, 'mbr-ml-