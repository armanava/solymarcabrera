        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "*": {
              "word-break": "break-word"
            },
            ".card-wrapper": {
              "overflow": "hidden"
            },
            ".card-wrapper > *:not(.mbr-overlay), .card-wrapper": {
              "position": "relative"
            },
            ".card-wrapper when not (@transparentBg)": {
              ".mbr-overlay": {
                "background-color": "@cardBg"
              },
              ".card-head": {
                "background-color": "fade(@secondaryColor, 12%)"
              }
            },
            ".card-wrapper when (@showBorder)": {
              "border": "1px solid @colorBorder"
            },
            ".amp-iconfont when (@showIcons)": {
              "font-size": "~\"@{iconsSize}rem\"",
              "width": "~\"@{iconsSize}rem\"",
              "color": "@iconsColor",
              "& when ((@iconsSize * 16) > 40)": {
                "@media (max-width: 767px)": {
                  "font-size": "40px",
                  "width": "40px"
                }
              }
            },
            ".card-price-wrap": {
              "align-items": "center",
              ".card-price": {
                "align-items": "flex-start"
              }
            },
            ".card when not (@transparentBg), (@showBorder)": {
              ".card-head": {
                "padding": "45px 35px 30px",
                "@media (max-width: 991px)": {
                  "padding": "25px 20px 20px"
                }
              },
              ".list-item": {
                "border-bottom": "1px solid",
                "& when not (@transparentBg)": {
                  "border-color": "darken(@cardBg, 9.2%)"
                },
                "& when (@showBorder)": {
                  "border-color": "@colorBorder"
                }
              },
              "&.md-pb when (@cardsAmount = 4)": {
                "@media (max-width: 991px)": {
                  "padding-bottom": "20px"
                }
              },
              "@media (max-width: 767px)": {
                "&:not(.last-child)": {
                  "padding-bottom": "20px"
                }
              },
              "& when (@showFavorite)": {
                "&.favoritePrice": {
                  ".card-wrapper:before": {
                    "content": "'BEST'",
                    "position": "absolute",
                    "right": "-25px",
                    "top": "10px",
                    "font-size": "12px",
                    "font-weight": "600",
                    "-webkit-transform": "rotate(45deg)",
                    "transform": "rotate(45deg)",
                    "background-color": "@primaryColor",
                    "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%)",
                    "padding": "5px 30px",
                    "line-height": "initial",
                    "z-index": "1"
                  },
                  ".card-head": {
                    "background-color": "@favoriteBgColor",
                    "> *, .amp-iconfont": {
                      "color": "@favoriteTextColor !important"
                    }
                  }
                },
                "&:not(.favoritePrice) .card-wrapper when (@showEffect) and not (@cardsAmount = '1')": {
                  ".card-head > *, .card-head, .amp-iconfont, svg": {
                    "-webkit-transition": "all 0.2s",
                    "transition": "all 0.2s"
                  },
                  "&:hover .card-head": {
                    "background-color": "@favoriteBgColor",
                    "> *, .amp-iconfont": {
                      "color": "@favoriteTextColor !important"
                    },
                    "svg": {
                      "fill": "@favoriteTextColor"
                    }
                  }
                }
              }
            },
            "& when (@showMilestone) and not (@titleAlign = '2')": {
              ".milestone, .milestone > div, .milestone > div > div": {
                "width": "100%"
              },
              ".milestone > div > div": {
                "padding-left": "0",
                "padding-right": "0"
              },
              ".milestone > div > div when (@showBorderBottom)": {
                "border-bottom": "1px solid @milBorderColor"
              },
              ".milestone .mbr-text": {
                "padding-top": "5px"
              },
              ".title .milestone > div when (@showTitle), (@showSubtitle)": {
                "& when (@milCount > 2)": {
                  "&:nth-child(1), &:nth-child(2)": {
                    "padding-bottom": "20px"
                  }
                }
              },
              "@media (max-width: 767px)": {
                ".title-wrap": {
                  "padding-bottom": "20px"
                },
                ".milestone > div:not(:last-child)": {
                  "padding-bottom": "20px"
                },
                ".milestone .numeral": {
                  "justify-content": "center"
                }
              }
            },
            ".card-price-wrap when (@cardAlign = '3')": {
              "@media (max-width: 767px)": {
                "justify-content": "center"
              }
            }
          },
          "_name": "pricing-table1",
          "_customHTML": "<section class=\"pricing-table1\" group=\"Pricing Tables\" plugins=\"amp-main\">\n\t<mbr-parameters>\n\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t\t<header>Size</header>\n\t\t<input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n\t\t<!--  -->\n\t\t<header>Title</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n            <input type=\"checkbox\" title=\"Milestone With Number\" name=\"showMilestone\" condition=\"titleAlign != 2 || (!showTitle && !showSubtitle)\" checked>\n                <input type=\"checkbox\" title=\"Border Bottom\" name=\"showBorderBottom\" condition=\"showMilestone && (titleAlign != 2 || (!showTitle && !showSubtitle))\">\n                <input type=\"color\" title=\"Border Color\" name=\"milBorderColor\" value=\"#bec1c7\" condition=\"showMilestone && showBorderBottom && (titleAlign != 2 || (!showTitle && !showSubtitle))\" checked>\n            <input type=\"range\" inline title=\"Count Milestone\" name=\"milCount\" min=\"1\" max=\"4\" step=\"1\" value=\"2\" condition=\"showMilestone && (titleAlign != 2 || (!showTitle && !showSubtitle))\">\n            <input type=\"checkbox\" title=\"Reverse Content Title\" name=\"reverseContent\" condition=\"showMilestone && (showTitle || showSubtitle) && titleAlign != 2\">\n            <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n                <option value=\"1\" selected>Left</option>\n                <option value=\"2\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n\t\t<header>Cards</header>\n\t\t<input ty