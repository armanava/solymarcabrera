            ".card-img amp-img": {
              "width": "~\"@{imageWidth}%\"",
              "max-width": "370px",
              "min-width": "60px"
            },
            ".card-subtitle:before when (@lineSpecialty) and (@showSpecialty)": {
              "content": "''",
              "width": "25px",
              "height": "3px",
              "display": "inline-block",
              "vertical-align": "middle",
              "margin-right": "10px",
              "background-color": "@primaryColor"
